# Google Drive API Configuration
# Instructions: Copy this file to .env and fill in your values

# Optional: Gemini API Key (if using Gemini instead of Bedrock)
# Get from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_gemini_api_key_here

# Optional: AWS Configuration (if using Bedrock)
AWS_REGION=us-east-1
AWS_PROFILE=default

# Cache Configuration
CACHE_ENABLED=true
CACHE_DIR=data/cache

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
